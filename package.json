{
  "homepage": "https://checkout.zksync.io",
  "name": "@matterlabs/zksync-dapp-checkout",
  "title": "zkCheckout â€” simple permissionless DeFi payment gateway",
  "description": "zkCheckout helps anyone permission-less adopt checkout backed by zkSync, receive payments automatically and benefit from all the advantage of zkSync Rollup: speed of the transaction, times lower cost of a single transaction, simplicity of withdrawal fund to the onchain-wallet.",
  "author": "Matter Labs",
  "private": true,
  "version": "2.2.0",
  "main": "nuxt.config.ts",
  "types": "src/types/index.d.ts",
  "files": [
    "src/**/*",
    "tests/*"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "bugs": {
    "url": "https://linear.app/matterlabs/project/zkcheckout-eae8f3c3e539/ZKF"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --format -E HUSKY_GIT_PARAMS"
    }
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://www.opensource.org/licenses/mit-license.php"
    },
    {
      "type": "Apache-2.0",
      "url": "https://opensource.org/licenses/apache2.0.php"
    }
  ],
  "lint-staged": {
    "*.{css,scss}": "yarn run lint-css",
    "*.{ts,tsx}": [
      "yarn run lint-ts",
      "yarn run format"
    ]
  },
  "scripts": {
    "ci:clear": "bash cli-dev.sh ci",
    "ci:prepare:rinkeby": "sh cli-process-env.sh 'rinkeby' dev 1",
    "ci:prepare:ropsten": "sh cli-process-env.sh 'ropsten' dev 1",
    "ci:prepare:mainnet": "sh cli-process-env.sh 'mainnet' dev 1",
    "ci:validate": "yarn ci && sh cli-process-env.sh 'mainnet' dev 1",
    "deploy:prod": "./cli-deploy-production.sh",
    "dev-beta": "sh cli-process-env.sh 'rinkeby-beta' 'dev' 1 1 && nuxt",
    "start": "sh cli-process-env.sh 'rinkeby' 'dev' && nuxt start",
    "build": "sh cli-process-env.sh 'mainnet' 'prod' true && nuxt build",
    "test": "jest",
    "fmt:check": "npx prettier --config .prettierrc.js --ignore-path .gitignore -c src/",
    "lint-ts:check": "eslint -c tsconfig.eslint.json -c .eslintrc.js --fix-dry-run --ext \".js,.ts,.vue\" src/**",
    "lint-style:check": "stylelint --config .stylelintrc  --ignore-path .stylelintrc src/",
    "lint-ts:fix": "eslint -c tsconfig.eslint.json -c .eslintrc.js --ext \".js,.ts,.vue\" --fix src/",
    "lint-style:fix": "stylelint --config .stylelintrc --ignore-path .stylelintrc src/",
    "ci": "rm -fr node_modules && rm -rf 'public/*' && sh ./cli-check-env.sh && yarn install --immutable  --immutable-cache",
    "ci:build": "yarn nuxt-ci && && sh ./cli-process-env.sh \"$NETWORK\" && yarn generate --fail-on-error",
    "dev": "sh ./cli-check-env.sh && nuxt",
    "generate": "sh ./cli-check-env.sh && nuxt generate ",
    "start:firebase": "firebase serve --only hosting",
    "prepare_ci": "yarn ci && sh cli-process-env.sh 'ropsten'",
    "prepare_ci_rinkeby": "yarn ci && sh cli-process-env.sh 'rinkeby'",
    "prepare_ci_mainnet": "yarn ci && sh cli-process-env.sh 'mainnet'",
    "build:prod": "yarn prepare_ci_mainnet && yarn generate --fail-on-error",
    "build:stage": "yarn prepare_ci_rinkeby && yarn generate --fail-on-error"
  },
  "dependencies": {
    "@matterlabs/zk-wallet-onboarding": "^1.18.6",
    "@nuxt/components": "2.1.6",
    "@nuxt/http": "^0.6.4",
    "@nuxt/typescript-runtime": "latest",
    "@nuxtjs/axios": "latest",
    "@nuxtjs/google-gtag": "^1.0.4",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/sentry": "latest",
    "@nuxtjs/tailwindcss": "^3.4.2",
    "@nuxtjs/toast": "latest",
    "@types/js-cookie": "^2.2.6",
    "@walletconnect/web3-provider": "^1.3.6",
    "ethereum-blockies-base64": "^1.0.2",
    "ethers": "^5.0.31",
    "js-cookie": "^2.2.1",
    "lottie-web": "^5.7.8",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "nuxt": "latest",
    "nuxt-build-optimisations": "^1.0.3",
    "nuxt-i18n": "^6.20.4",
    "nuxt-social-meta": "^0.0.5",
    "nuxt-typed-vuex": "latest",
    "nuxt-webfontloader": "^1.1.0",
    "oh-vue-icons": "^0.4.3",
    "vue": "latest",
    "vue-qrcode": "^0.3.5",
    "vue-scrollto": "^2.20.0",
    "vuex": "^3.6.2",
    "web3": "^1.3.5",
    "zksync": "latest",
    "zksync-checkout": "^0.0.11",
    "zksync-checkout-internal": "^0.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.9",
    "@babel/runtime": "^7.13.10",
    "@ethersproject/logger": "^5.1.0",
    "@ethersproject/providers": "^5.1.0",
    "@inkline/nuxt": "^2.3.6",
    "@nuxt/types": "latest",
    "@nuxt/typescript": "latest",
    "@nuxt/typescript-build": "latest",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/eslint-config-typescript": "^6.0.0",
    "@nuxtjs/google-fonts": "^1.3.0",
    "@nuxtjs/style-resources": "latest",
    "@types/jest": "^26.0.22",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "fork-ts-checker-webpack-plugin": "^6.2.0",
    "jest": "^26.6.3",
    "node-sass": "4.14.0",
    "prettier": "^2.2.1",
    "qrcode": "^1.0.0",
    "sass-loader": "10.1.1",
    "stylelint": "^13.12.0",
    "stylelint-config-recommended": "^4.0.0",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-scss": "^3.19.0",
    "typed-vuex": "latest",
    "typescript": "^4.2.4",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vuex": "3.5.1",
    "web3-core": "^1.3.5",
    "webpack": "4.46.0"
  },
  "peerDependencies": {
    "@babel/core": "*",
    "@babel/runtime": "*",
    "typed-vuex": "*",
    "vuex": "3.5.1"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "fork-ts-checker": {
    "typescript": true
  },
  "optionalDependencies": {
    "fs-extra": "^9.1.0",
    "fsevents": "^2.3.2"
  }
}
